{
  "permissions": {
    "allow": [
      "Bash(npm run test:)",
      "Bash(git diff:)",
      "Read(./*.md)",
      "Read(./*.json)"
    ],
    "ask": [
      "Bash(git push:)",
      "Write(/)"
    ],
    "deny": [
      "Bash(curl:)",
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(./secrets/)"
    ]
  }
}
